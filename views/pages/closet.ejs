<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head', {title: title}); %>
</head>
<body onload="getCloset('<%= userId %>')">
    <header>
        <%- include('../partials/header'); %>
    </header>
    <main>
        <!-- Display message -->
        <div id="message_fade"><%= message %></div>
        
        <h1>My Closet</h1>
        <div id="closet_output">
            
        </div>
        <form action="/add" method="GET">
            <input class="button" type="submit" value="Add New Item">
        </form>
    </main>
    <footer>
        <%- include('../partials/footer', {dateString: dateString}); %>
    </footer>
</body>
</html>